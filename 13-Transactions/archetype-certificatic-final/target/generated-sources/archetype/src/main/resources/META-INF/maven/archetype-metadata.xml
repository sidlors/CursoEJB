<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="archetype-certificatic-final"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modules>
    <module id="jee-ear" dir="jee-ear" name="jee-ear" />
    <module id="jee-ejb" dir="jee-ejb" name="jee-ejb" />
    <module id="jee-war" dir="jee-war" name="jee-war">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/webapp</directory>
          <includes>
            <include>**/*.xhtml</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="jee-ejb-api" dir="jee-ejb-api" name="jee-ejb-api" />
    <module id="jee-jpa" dir="jee-jpa" name="jee-jpa" />
  </modules>
</archetype-descriptor>
